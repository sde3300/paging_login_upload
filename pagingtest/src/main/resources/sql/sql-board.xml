<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.btc.mapper.PageMapper">

    <select id="findUser" resultType="com.btc.dto.PageDto">
        <![CDATA[
            SELECT
            	first_name, last_name 
            FROM
            	employees
            WHERE
            	first_name like '%mario%'
        ]]>
    </select>
    
    <!-- id : mapper 파일에서 지정한 메서드명 -->
    <!-- parameterType : mapper 파일에서 지정한 메서드의 매개변수 타입 -->
    <!-- resultType : mapper 파일에서 지정한 메서드의 반환 타입 -->

</mapper>










